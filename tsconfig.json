{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": ["react", "react-native"],
    "baseUrl": "./",
    "paths": {
      "@assests/*": ["src/assests/*"],
      "@components/*": ["src/components/*"],
      "@context/*": ["src/context/*"],
      "@services/*": ["src/services/*"],
      "@models/*": ["src/models/*"],
      "@routes/*": ["src/routes/*"],
      "@viewmodel/*": ["src/viewmodel/*"]
    }
  },
}
